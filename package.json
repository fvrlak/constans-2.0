{
  "name": "constans",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10.11.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "css": "node-sass --output-style compressed ./public/styles/style.scss > ./public/styles/style.css",
    "scss": "node-sass -w public/styles/style.scss public/styles/style.css --",
    "nodemon": "nodemon server.js",
    "webpack": "webpack --mode development ./public/js/index.js --output ./public/bundle.js ",
    "webpack-watch": "webpack --mode development --watch ./public/js/index.js --output ./public/bundle.js",
    "startDev": "npm-run-all --parallel css  nodemon scss webpack webpack-watch",
    "start": "node server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "d3": "^5.7.0",
    "express": "^4.16.3",
    "hbs": "^4.0.1",
    "hbs-utils": "0.0.4",
    "node-sass": "^4.9.3"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "sass-loader": "^7.1.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
